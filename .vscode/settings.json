{
  "rust-analyzer.linkedProjects": ["./src/backend/Cargo.toml", "./Cargo.toml"],
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "prettier.documentSelectors": ["**/*.astro", "**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx", "**/*.css", "**/*.scss", "**/*.json", "**/*.md"],
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "nesseccery"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ccaffd",
    "activityBar.background": "#ccaffd",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#da5405",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ccaffd",
    "statusBar.background": "#ac7dfc",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#8c4bfb",
    "statusBarItem.remoteBackground": "#ac7dfc",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#ac7dfc",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ac7dfc99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#ac7dfc",
}
